<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                              http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
      <id>regression_properties</id>
      <properties>
        <tests.network>true</tests.network>
        <es.action.wait_on_mapping_change>true</es.action.wait_on_mapping_change>
        <tests.rest.blacklist>cat.recovery/10_basic/*</tests.rest.blacklist>
        <elasticsearch.thirdparty.config>shaded</elasticsearch.thirdparty.config>
        <tests.slow>true</tests.slow>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>default</activeProfile>
    <activeProfile>regression_properties</activeProfile>
  </activeProfiles>
</settings>
